{
    "name": "medcognito",
    "version": "1.0",
    "description": "Open Medium Articles in incognito mode in just one Click!",
    "manifest_version": 2,
    "background": {
        "scripts": ["script.js"]
    },
    "icons": { 
        "16": "icons8-medium-16.png",
        "48": "icons8-medium-48.png",
       "128": "icons8-medium-128.png" 
    },
    "browser_action": {
        "default_title": "medcognito"    
      }
      ,
      "permissions": [ 
        "tabs",
        "contextMenus"
     ],
     "commands": {
        "medcognito": {
          "suggested_key": {
            "default": "Ctrl+M"
          },
          "description": "Go Medcognito"
        }
     }
  }